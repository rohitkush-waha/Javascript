Variable Hoisting

console.log(x);//undefined
var x =5;
console.log(x);//5
////////////////////////////////////
console.log(y);
let y =10;
/////////////////////////////////////
console.log(PI);
const PI =3.14;
//////////////////////////////////////



///////////////////////////////////////
Function Hoisting

sayHello();

function sayHello(){
  Console.log("Hello, World!");
}
////////////////////////////////////////
